"VertexLitGeneric"
{
	"$basetexture"	"phunbase\attachments\fas2_watch_realtime\hand_plane_sec"
	"$translucent" "1"
	"$color2" "[0.55 0.3 0.2]"
	"$halflambert" "1"
	"$rimlight" "1"
	
	"$angle" 0.0
	"$translate" "[0 0]"
	"$center" "[0.5 0.5]"

	"Proxies"
	{
		"FAS2_WATCH_REALTIME_Seconds"
		{
			"resultVar" "$angle"
		}

		"TextureTransform"
		{
			"translateVar" "$translate"
			"rotateVar" "$angle"
			"centerVar" "$center"
